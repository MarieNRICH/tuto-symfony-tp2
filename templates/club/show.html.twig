{% extends 'base.html.twig' %}

{% block title %}Club{% endblock %}

{% block body %}
    <h1>Club</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ club.id }}</td>
            </tr>
            <tr>
                <th>ClubName</th>
                <td>{{ club.clubName }}</td>
            </tr>
            <tr>
                <th>SportClub</th>
                <td>{{ club.sportClub }}</td>
            </tr>
            <tr>
                <th>ClubAddress</th>
                <td>{{ club.clubAddress }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_club_index') }}">back to list</a>

    <a href="{{ path('app_club_edit', {'id': club.id}) }}">edit</a>

    {{ include('club/_delete_form.html.twig') }}
{% endblock %}
